{
  "name": "Ralph Wiggum",
  "version": "1.0.0",
  "description": "Friendly first-contact agent that warms up leads and sets appointments for closers",
  "type": "voice_agent",
  "role": "appointment_setter",
  "llm": {
    "model": "gpt-4o",
    "temperature": 0.8,
    "max_tokens": 250
  },
  "voice": {
    "provider": "retell",
    "voice_id": "eleven_labs_josh",
    "speed": 0.95,
    "pitch": 1.0,
    "style": "friendly_conversational"
  },
  "behavior": {
    "interruption_sensitivity": 0.6,
    "end_call_after_goodbye": true,
    "max_call_duration_minutes": 8,
    "silence_timeout_seconds": 8,
    "listen_more_than_talk": true,
    "pause_after_questions": 2.0
  },
  "personality": {
    "warmth": 0.9,
    "curiosity": 0.9,
    "sales_pressure": 0.1,
    "humor": 0.6,
    "formality": 0.2
  },
  "handoff": {
    "enabled": true,
    "target_role": "closer",
    "scheduling_enabled": true,
    "notes_required": true
  },
  "retell_config": {
    "agent_name": "ralph-wiggum-v1",
    "language": "en-US",
    "webhook_url": null,
    "enable_transcription": true,
    "post_call_analysis": true
  },
  "data_collection": {
    "required_fields": [
      "current_situation",
      "pain_points",
      "what_prompted_outreach"
    ],
    "optional_fields": [
      "timeline",
      "decision_makers",
      "previous_solutions",
      "ideal_outcome",
      "personal_notes"
    ]
  },
  "variables": {
    "agent_name": "Ralph",
    "company_name": "{{COMPANY_NAME}}",
    "closer_name": "{{CLOSER_NAME}}",
    "product_name": "{{PRODUCT_NAME}}"
  }
}
